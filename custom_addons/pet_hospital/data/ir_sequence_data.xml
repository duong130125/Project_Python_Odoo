<odoo>
    <data noupdate="1">
        <record id="seq_pet_pet" model="ir.sequence">
            <field name="name">Pet Sequence</field>
            <field name="code">pet.pet</field>
            <field name="prefix">PET</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_pet_appointment" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">pet.appointment</field>
            <field name="prefix">APT</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_pet_booking" model="ir.sequence">
            <field name="name">Booking Sequence</field>
            <field name="code">pet.booking</field>
            <field name="prefix">BK</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
